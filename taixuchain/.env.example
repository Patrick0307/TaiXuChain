# ============================================
# TaiXu Frontend Environment Configuration
# Copy this file to .env and fill in your values
# ============================================

# Smart Contract Configuration (update after deployment)
VITE_PACKAGE_ID=your_package_id_here
VITE_REGISTRY_ID=your_registry_id_here
VITE_MARKETPLACE_ID=your_marketplace_id_here
VITE_MARKET_ID=your_marketplace_id_here
VITE_WEAPON_MINT_CAP=your_weapon_mint_cap_id_here

# LingStone Configuration
VITE_LINGSTONE_PACKAGE_ID=your_package_id_here
VITE_LINGSTONE_TREASURY_CAP=your_treasury_cap_id_here

# Sponsor Wallet Address (pays gas for sponsored transactions)
VITE_SPONSOR_ADDRESS=your_sponsor_address_here

# Game Treasury Address (receives LingStone payments)
VITE_GAME_TREASURY_ADDRESS=your_game_treasury_address_here

# Backend API (Local Development)
VITE_BACKEND_URL=http://localhost:3001

# Backend API (Production - uncomment when deploying)
# VITE_BACKEND_URL=https://your-backend.onrender.com
# VITE_WS_URL=wss://your-backend.onrender.com
